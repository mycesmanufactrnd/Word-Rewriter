{
  "manifest_version": 3,
  "name": "Word Auto Rewriter",
  "version": "1.0",
  "description": "Automatically rewrite words to synonyms.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["synonyms.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
